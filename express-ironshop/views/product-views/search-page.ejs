<h2> Search Results </h2>

<p>
  You Searched for <b> <%= searchTerm %> </b>.
  We Found <%= listOfResults.length %> Results.
</p>

<ul>
  <% listOfResults.forEach((oneProduct) => { %>
    <li>
      <h3> <%= oneProduct.name %> </h3>
      <p> <%= oneProduct.price %> </p>
      <img src="<%= oneProduct.imageURL %>" />
      <br />

      <a href="/products/ <%= oneProduct._id %>">
        View Details
      </a>
    </li>
  <% }) %>
</ul>
