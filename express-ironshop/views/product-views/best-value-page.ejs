<h2> Best Values Products </h2>

<ol>
  <% listOfCheapProducts.forEach((oneProduct) => { %>
    <li>
      <h3> <%= oneProduct.name %> </h3>
      <p> <%= oneProduct.price %> </p>
      <img src="<%= oneProduct.imageURL %>" />
      <br />

      <a href="/products/ <%= oneProduct._id %>">
        View Details
      </a>
    </li>
  <% }) %>
</ol>
